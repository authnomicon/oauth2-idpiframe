{
  "name": "@authnomicon/oauth2-idpiframe",
  "version": "0.0.0",
  "description": "OAuth 2.0 IDP IFrame components for the Authnomicon project.",
  "author": {
    "name": "Jared Hanson",
    "email": "jaredhanson@gmail.com",
    "url": "https://www.jaredhanson.me/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/authnomicon/oauth2-idpiframe.git"
  },
  "bugs": {
    "url": "https://github.com/authnomicon/oauth2-idpiframe/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jaredhanson"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "main": "./app",
  "assembly": {
    "namespace": "org.authnomicon/oauth2/idpiframe",
    "components": [
      "oauth2/authorize/http/response/types/permission",
      "oauth2/authorize/http/response/schemes/storagerelay",
      "oauth2/authorize/http/response/modes/iframerpc",
      "iframe/http/service",
      "rpc/http/service",
      "id/loginhint/default"
    ],
    "x_components": [
      "http/iframe",
      "http/rpc",
      "ext/modes/iframerpc",
      "implicit/permission"
    ]
  },
  "directories": {
    "com": "com"
  },
  "dependencies": {
    "array-flatten": "^1.1.1",
    "express": "^4.17.1",
    "oauth2orize": "^1.11.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "chai-express-handler": "^0.1.1",
    "make-node": "^0.3.0",
    "mocha": "^2.0.0",
    "proxyquire": "^2.1.3",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha test/*.test.js"
  }
}
